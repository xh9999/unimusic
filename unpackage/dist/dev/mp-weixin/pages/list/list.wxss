.container .title {
  background-color: #222;
  text-align: center;
  color: #fff;
  padding: 20rpx 0;
}
.container .uni-list-item {
  background-color: #000;
  color: #fff;
  font-size: 30rpx;
}
.container .icon {
  width: 110rpx;
  height: 110rpx;
  margin-right: 40rpx;
}
.container .icon image {
  width: 110rpx;
  height: 110rpx;
}
.container .content {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.container .content .one {
  margin-top: 10rpx;
}
.container .mv {
  background-color: #000;
  color: aliceblue;
  font-size: 30rpx;
}
.container .mv .img {
  border-radius: 20rpx;
}
.container .mv .img image {
  border: solid 1px #888;
  border-radius: 30rpx;
  width: 100%;
}
.container .mv .mv-cot {
  padding: 0 30rpx;
  height: 100rpx;
}
.container .mv .mv-cot view {
  padding-top: 10rpx;
}
.container .mv .mv-cot view:first-child {
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.container .mv .mv-cot view:last-child {
  font-size: 24rpx;
}

