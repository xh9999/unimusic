.item {
  background-color: black;
}
.item .item-list {
  width: 100%;
  height: 110rpx;
  position: relative;
}
.item .item-list .item-text {
  position: absolute;
  top: 35rpx;
  left: 90rpx;
  font-size: 30rpx;
  color: white;
}
.item .item-list .item-img {
  width: 70rpx;
  height: 70rpx;
  position: absolute;
  top: 20rpx;
  border-radius: 10rpx;
}

