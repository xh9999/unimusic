page {
  height: 100%;
  position: relative;
}
page .message {
  text-align: center;
}
page .container {
  height: 100%;
}
page .mask {
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.5;
}
page .center {
  padding: 20rpx;
  width: 80%;
  position: absolute;
  top: 40%;
  left: 50%;
  -webkit-transform: translate(-50%, -60%);
          transform: translate(-50%, -60%);
  background-color: #fff;
  z-index: 2;
  border-radius: 20rpx;
  overflow: hidden;
  height: 400rpx;
}
page .center .one .uni-forms-item {
  margin-top: 40rpx;
}
page .center .one .uni-forms-item__label {
  padding-top: 10rpx;
}
page .center .two {
  margin-top: 30rpx;
}
page .center button {
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 300rpx;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}

